{
	"todohighlight.isEnable": true,
	"todohighlight.isCaseSensitive": true,
	"todohighlight.keywords": [
		{
			"text": "Section:",
			"backgroundColor": "#AFA2FF",
			"color": "#E3D7FF"
		},
		{
			"text": "_UserSession",
			"color": "#DEA47E",
			"backgroundColor": "#9E6240"
		},
		{
			"text": "Mutations:",
			"backgroundColor": "#1A535C",
			"color": "#4ECDC4"
		},
		{
			"text": "Queries:",
			"color": "#d8f1a0",
			"backgroundColor": "#00a878"
		},
		{
			"text": "Test:",
			"backgroundColor": "#f55d3e",
			"color": "#f5efed"
		},
		{
			"text": "Define:",
			"backgroundColor": "#ECC8AE",
			"color": "#DDFFD9"
		},
		{
			"text": "// ",
			"backgroundColor": "#84ACCE",
			"color": "#D7D9B1"
		},
		{
			"text": "_Find",
			"backgroundColor": "#62B6CB",
			"color": "#BEE9E8"
		},
		{
			"text": "_Editor",
			"backgroundColor": "#B1B695",
			"color": "#FCD0A1"
		},
		{
			"text": "_User",
			"backgroundColor": "#60992D",
			"color": "#F7F0F5"
		},
		{
			"text": "_Token",
			"backgroundColor": "#2E294E",
			"color": "#F46036"
		},
		{
			"text": "_Todo",
			"color": "#FBFFFE",
			"backgroundColor": "#FAA916"
		},
		{
			"text": "_Data",
			"color": "#ACBFA4",
			"backgroundColor": "#616163"
		}
	],
	"todohighlight.include": [
		"**/*.js",
		"**/*.jsx",
		"**/*.ts",
		"**/*.tsx",
		"**/*.html",
		"**/*.php",
		"**/*.css",
		"**/*.scss"
	],
	"todohighlight.exclude": [
		"**/node_modules/**",
		"**/bower_components/**",
		"**/dist/**",
		"**/build/**",
		"**/.vscode/**",
		"**/.github/**",
		"**/_output/**",
		"**/*.min.*",
		"**/*.map",
		"**/.next/**"
	],
	"todohighlight.maxFilesForSearch": 5120,
	"todohighlight.toggleURI": false
}
